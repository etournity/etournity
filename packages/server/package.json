{
  "name": "@etournity/server",
  "version": "1.6.1",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build": "yarn generate && yarn tsc --outDir dist",
    "dev": "yarn prisma db push && ts-node-dev --transpile-only src/index.ts",
    "generate": "yarn prisma generate && yarn generate:nexus",
    "generate:nexus": "ts-node --transpile-only src/schema.ts --nexus-exit",
    "migrate": "yarn prisma migrate dev",
    "start": "ts-node --transpile-only src/index.ts",
    "db:seed": "ts-node --transpile-only src/seed/justSeed.ts",
    "test": "exit 0",
    "test:types": "yarn tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@etournity/rbac": "^1.6.1",
    "@etournity/shared": "^1.6.1",
    "@graphql-tools/schema": "^8.3.2",
    "@prisma/client": "3.13.0",
    "@sentry/node": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "@tsconfig/node12": "1.0.7",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "1.4.2",
    "@types/cron": "^1.7.2",
    "@types/express-session": "^1.17.4",
    "@types/ioredis": "^4.27.3",
    "@types/ms": "0.7.31",
    "@types/passport": "1.0.4",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-discord": "0.1.3",
    "@types/passport-jwt": "3.0.3",
    "@types/ws": "^8.5.2",
    "apollo-server-express": "3.6.3",
    "bull": "^4.5.1",
    "client-oauth2": "4.3.3",
    "connect-redis": "^6.0.0",
    "cookie-parser": "1.4.5",
    "cron": "^1.8.2",
    "express": "4.17.3",
    "express-session": "^1.17.2",
    "graphql": "^15.4.0",
    "graphql-middleware": "6.1.15",
    "graphql-query-complexity": "^0.7.2",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-shield": "7.4.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.6.2",
    "helmet": "4.2.0",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "^8.5.1",
    "mixpanel": "^0.14.0",
    "ms": "2.1.2",
    "nexus": "^1.3.0",
    "nexus-prisma": "0.34.0",
    "passport": "0.4.1",
    "passport-anonymous": "1.0.1",
    "passport-discord": "0.1.4",
    "passport-jwt": "4.0.0",
    "prisma": "3.13.0",
    "rate-limiter-flexible": "^2.1.13",
    "ts-node": "10.2.1",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.3",
    "ws": "^8.5.0",
    "yaml": "1.10.0"
  },
  "devDependencies": {
    "@types/bull": "^3.15.7",
    "@types/mixpanel": "^2.14.3",
    "ts-node-dev": "1.1.8"
  }
}
